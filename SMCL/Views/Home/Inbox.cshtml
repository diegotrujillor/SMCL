<table id="inboxDataTable" class="display">
	<thead>
		<tr>
			<th style="width: 20%; padding-left: 2%; font-weight: bold;">@ViewBag.DateTitle</th>
            <th style="width: 15%; padding-left: 2%; font-weight: bold;">@ViewBag.Appliance</th>
            <th style="width: 15%; padding-left: 2%; font-weight: bold;">@ViewBag.AlarmType</th>
            <th style="width: 15%; padding-left: 2%; font-weight: bold;">@ViewBag.Value</th>
            <th style="width: 33%; padding-left: 2%; font-weight: bold;">@ViewBag.Variable</th>
            <th style="width: 2%; padding-left: 2%; font-weight: bold;">@String.Empty</th>
		</tr>
	</thead>
	<tbody>
	</tbody>
</table>

<script type="text/javascript" charset="utf-8">
    $(document).ready(function () {
        $('#inboxDataTable').dataTable({
            "bFilter": false,
            "bProcessing": true,
            "bServerSide": true,
            "iDisplayLength": 6,
            "sAjaxSource": "../Home/FillGridReturnList",
            "sPaginationType": "full_numbers",
            "oLanguage": {
                "sUrl": "../Content/es-CO.txt"
            },
            "aoColumns": [
                            { "sName": "SENDER",
                              "bSortable": false },
                            { "sName": "SUBJECT",
                                "bSortable": false },
                            { "sName": "DATE",
                                "bSortable": false },
                            {
                                "sName": "ID",
                                "bSearchable": false,
                                "bSortable": false,
                                "fnRender": function (oObj) {
                                    return '<a href=\"#\"><img src=\"../Content/Images/3edit.png\" alt=\"edit\" title=\"Reconocer\" onclick=\"overlay(' + oObj.aData[3] + ')\" /></a>';
                                }
                            }
                        ]
        });
    });
</script>