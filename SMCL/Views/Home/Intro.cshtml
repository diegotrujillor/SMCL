@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.IntroTitle</title>    
    <link href="@Url.Content("~/Content/Intro.css")" rel="Stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/DD_roundies_0.0.2a-min.js")" type="text/javascript"></script>
    <link rel="Shortcut Icon" href="@Url.Content("~/Content/Images/johnson_and_johnson_no_bg.ico")" />
</head>
<body>
    <table id="main_logo">
        <tr>
            <td>
                <a href="http://www.jnjcolombia.com.co" target="_blank">
                    <img src="@Url.Content("~/Content/Images/jnj-blanco-title.png")" alt="J&J" title="Johnson & Johnson de Colombia S.A" width="60%" border="0" style="float: right;" />
                </a>
            </td>
        </tr>
    </table>
    
    <a href="@Url.Content("~/Home/LogOn")" class="myButton">@ViewBag.IntroButtonText</a>
</body>
</html>

<script type="text/javascript">
    DD_roundies.addRule('.myButton', '14px');
</script>