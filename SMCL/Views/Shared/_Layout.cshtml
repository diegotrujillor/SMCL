<!DOCTYPE html>
<html>
<head>
	<title>@ViewBag.Title</title>
	<link rel="Shortcut Icon" href="@Url.Content("~/Content/Images/johnson_and_johnson_no_bg.ico")" />

	@*CSS Files*@
	<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
	<link href="@Url.Content("~/Content/fisheye.css")" rel="stylesheet" type="text/css" />
	<link href="@Url.Content("~/Content/jquery-ui-1.8.7.css")" rel="stylesheet" type="text/css" />
	<link href="@Url.Content("~/Content/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
	<link href="@Url.Content("~/Content/ui.multiselect.css")" rel="stylesheet" type="text/css" />
	<link href="@Url.Content("~/Content/jquery.dataTables.css")" rel="Stylesheet" type="text/css" />
	<link href="@Url.Content("~/Content/PasswordMeter/jquery.passwordMeter.css")" rel="Stylesheet" type="text/css" />
	<!--[if lt IE 7]>
		<link href="@Url.Content("~/Content/PasswordMeter/ie.css")" rel="Stylesheet" type="text/css" />
	<![endif]-->
	
	<style type="text/css">
		@*fix the size of the pager*@
		input.ui-pg-input{ width: auto;}
	</style>

	@*DataTable Inbox*@
	<script src="@Url.Content("~/Scripts/jquery.js")" type="text/javascript"></script>
	<script src="@Url.Content("~/Scripts/jquery.dataTables.js")" type="text/javascript" />
	
	@*jQuery Library*@
	<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
	<script src="@Url.Content("~/Scripts/interface.js")" type="text/javascript"></script>

	@*Must load language tag BEFORE script tag*@
	<script src="@Url.Content("~/Scripts/grid.locale-en.js")" type="text/javascript"></script>
	<script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
</head>
<body>
	<div class="page">
		<div id="header">
			<div id="title">                
			</div>
			<div id="menucontainer">
				<table id="no_css" style="width: 100%; table-layout: fixed;">
					<tr>
						<td style="width: 25%">
							<a href="http://www.jnjcolombia.com.co" target="_blank">
								<img src="@Url.Content("~/Content/Images/jnj-blanco.png")" alt="J&J" title="Johnson & Johnson de Colombia S.A" width="99%" height="60px" border="0" />
							</a>
						</td>
						<td  style="width: 65%">
							<h2 style="color: #FFF; font-size: x-large; padding-left: 2%; text-align: center;">
								@System.Configuration.ConfigurationManager.AppSettings["HeaderMessage"].ToString()
							</h2>
						</td>
						<td  style="width: 10%">
							<img src="@Url.Content("~/Content/Images/qa_jyj.png")" alt="QA" title="Quality Assurance Latin America" width="90%" style="padding-top: 5%;" />
						</td>
					</tr>
				</table>
				<ul id="menu">
					<li>@Html.ActionLink(System.Configuration.ConfigurationManager.AppSettings["IndexMenuLabel"], "Index", "Home")</li>
					<li>@Html.ActionLink(System.Configuration.ConfigurationManager.AppSettings["MapMenuLabel"], "Map", "Home")</li>
					<li>@Html.ActionLink(System.Configuration.ConfigurationManager.AppSettings["ReportingMenuLabel"], "Reporting", "Home")</li>
					<li>@Html.ActionLink(System.Configuration.ConfigurationManager.AppSettings["ManagementMenuLabel"], "Management", "Home")</li>
					<li>@Html.ActionLink(System.Configuration.ConfigurationManager.AppSettings["LogMenuLabel"], "Index", "Log")</li>
					<li>@Html.ActionLink(System.Configuration.ConfigurationManager.AppSettings["AboutMenuLabel"], "About", "Home")</li>
					<li>@Html.ActionLink(System.Configuration.ConfigurationManager.AppSettings["LogOffMenuLabel"], "LogOff", "Home")</li>
				</ul>
			</div>
		</div>
		<div id="main">
			@RenderBody()
		</div>
		<table id="no_css" align="right" style="table-layout: fixed;">
			<tr>
				<td style="width: 5%;">
					<a href="http://validator.w3.org/check?uri=referer"><img
							src="@Url.Content("~/Content/Images/jnj_valid_xhtml10.png")"
							alt="Valid XHTML 1.0 Transitional" height="22" width="62"
							border="0" /> </a>
				</td>
				<td style="width: 5%;">
					<a href="http://jigsaw.w3.org/css-validator/check/referer">
						<img style="border: 0; width: 62px; height: 22px" border="0"
							src="@Url.Content("~/Content/Images/jnj_valid_css.gif")"
							alt="Valid CSS!" /> </a>
				</td>
				<td style="width: 5%;">
					<a href="http://validator.w3.org/" target="_blank"> <img
							src="@Url.Content("~/Content/Images/jnj_valid_html5_badge_h_css3_semantics.png")"
							width="62" height="24" border="0"
							alt="HTML5 Powered with CSS3 / Styling, and Semantics"
							title="HTML5 Powered with CSS3 / Styling, and Semantics" />
					</a>
				</td>
				<td style="width: 85%; text-align: right;">
					<img src="@Url.Content("~/Content/Images/1.gif")" alt="bullet-1" />
					<label style="font-family: verdana; color: #00ffff;">
						@System.Configuration.ConfigurationManager.AppSettings["FooterMessage1"].ToString()
						<a href="@System.Configuration.ConfigurationManager.AppSettings["FooterMessage1Link"].ToString()" target="_blank" style="color: #00ffff; text-decoration: none; font-weight: bold; font-family: verdana;">
							@System.Configuration.ConfigurationManager.AppSettings["FooterMessage2"].ToString()
						</a>
						<br />
						@System.Configuration.ConfigurationManager.AppSettings["FooterMessage3"].ToString()<b>@System.Configuration.ConfigurationManager.AppSettings["FooterMessage4"].ToString()</b>
					</label>
				</td>
			</tr>
		</table>
	</div>
</body>
</html>
